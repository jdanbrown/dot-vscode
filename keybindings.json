  {"key": "ctrl+cmd+[", "command": "multiCommand.addHighlightAnd.editor.action.referenceSearch.trigger", "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"},

