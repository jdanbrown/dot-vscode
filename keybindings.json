  {"key": "ctrl+k", "command": "editor.action.insertCursorAbove", "when": "editorTextFocus && !inSearchEditor && editorLangId != 'magit'"},
  {"key": "alt+cmd+down", "command": "-editor.action.insertCursorBelow", "when": "editorTextFocus"},
