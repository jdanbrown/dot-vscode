  {"key": "cmd+u", "command": "-cursorUndo", "when": "textInputFocus"},

  // Search Editor
  //  - https://github.com/VSCodeVim/Vim/issues/6619 Focus next search result enters visual mode
  //  - https://github.com/VSCodeVim/Vim/issues/3372 How to disable Visual Mode in VSCodeVim?
  //  - https://github.com/VSCodeVim/Vim/issues/7254 <C-Up> and <C-Down> cause editor to enter Visual mode #7254
  //  - https://github.com/VSCodeVim/Vim/issues/5716 Select Mode
  // {"key": "shift+cmd+f", "command": "search.action.openNewEditorToSide"}, // -> livegrep -> Quick Search
  {"key": "shift+cmd+f", "command": "-workbench.view.search", "when": "!searchViewletVisible"},
  {"key": "shift+cmd+f", "command": "-workbench.action.findInFiles"},
  {"key": "cmd+f", "command": "search.action.focusQueryEditorWidget", "when": "inSearchEditor && !searchInputBoxFocus"},
  {"key": "/", "command": "search.action.focusQueryEditorWidget", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && !referenceSearchVisible"},
  {"key": "cmd+i", "command": "search.action.focusFilesToInclude", "when": "inSearchEditor"},
  {"key": "cmd+e", "command": "search.action.focusFilesToExclude", "when": "inSearchEditor"},
  {"key": "cmd+f", "command": "multiCommand:search.action.unfocusQueryEditorWidget", "when": "inSearchEditor && searchInputBoxFocus"},
  {"key": "enter", "command": "multiCommand:search.action.unfocusQueryEditorWidget", "when": "inSearchEditor && inputBoxFocus"},
  {"key": "escape", "command": "multiCommand:search.action.unfocusQueryEditorWidget", "when": "inSearchEditor && inputBoxFocus"},
  {"key": "escape", "command": "-search.action.focusQueryEditorWidget", "when": "inSearchEditor"},
  {"key": "cmd+g", "command": "search.action.focusNextSearchResult", "when": "inSearchEditor && !referenceSearchVisible"},
  {"key": "shift+cmd+g", "command": "search.action.focusPreviousSearchResult", "when": "inSearchEditor && !referenceSearchVisible"},
  {"key": "cmd+g", "command": "multiCommand:search.action.focusNextSearchResult+editor.action.peekDefinition", "when": "inSearchEditor && referenceSearchVisible"},
  {"key": "shift+cmd+g", "command": "multiCommand:search.action.focusPreviousSearchResult+editor.action.peekDefinition", "when": "inSearchEditor && referenceSearchVisible"},
  //  - Use opt+[hjkl] (arrow keys) to move cursor
  {"key": "j", "command": "search.action.focusNextSearchResult", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && !referenceSearchVisible"},
  {"key": "k", "command": "search.action.focusPreviousSearchResult", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && !referenceSearchVisible"},
  {"key": "j", "command": "multiCommand:search.action.focusNextSearchResult+editor.action.peekDefinition", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && referenceSearchVisible"},
  {"key": "k", "command": "multiCommand:search.action.focusPreviousSearchResult+editor.action.peekDefinition", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && referenceSearchVisible"},
  {"key": "shift+cmd+i", "command": "workbench.action.search.toggleQueryDetails", "when": "inSearchEditor && !inputBoxFocus"},
  {"key": "shift+cmd+e", "command": "workbench.action.search.toggleQueryDetails", "when": "inSearchEditor && !inputBoxFocus"},
  {"key": "shift+cmd+i", "command": "multiCommand:search.action.focusQueryEditorWidget+workbench.action.search.toggleQueryDetails", "when": "inSearchEditor && inputBoxFocus"}, // Workaround focus trap
  {"key": "shift+cmd+e", "command": "multiCommand:search.action.focusQueryEditorWidget+workbench.action.search.toggleQueryDetails", "when": "inSearchEditor && inputBoxFocus"}, // Workaround focus trap
  {"key": "ctrl+r", "command": "rerunSearchEditorSearch", "when": "inSearchEditor"},
  {"key": "ctrl+c", "command": "toggleSearchEditorCaseSensitive", "when": "inSearchEditor"},
  {"key": "ctrl+w", "command": "toggleSearchEditorWholeWord", "when": "inSearchEditor"},
  {"key": "ctrl+/", "command": "toggleSearchEditorRegex", "when": "inSearchEditor"},
  {"key": "=", "command": "increaseSearchEditorContextLines", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && !referenceSearchVisible"},
  {"key": "-", "command": "decreaseSearchEditorContextLines", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/ && !referenceSearchVisible"},
  {"key": "backspace", "command": "search.searchEditor.action.deleteFileResults", "when": "inSearchEditor && !inputBoxFocus"},
  {"key": "shift+cmd+backspace", "command": "-search.searchEditor.action.deleteFileResults", "when": "inSearchEditor"}, // Confusion with File Delete
  //  - To open results, use Peek Definition / Go To Definition
  //    - TODO(practice)
  //    - https://code.visualstudio.com/docs/editor/codebasics#_search-editor
  //      - Use Goto/Peek on the results
  //    - https://stackoverflow.com/questions/63106163/vscode-navigate-search-editor-using-keyboard-only/63107297#63107297
  //      - Use Goto/Peek on the filename headers to peek all of its results at once -- useful!
  //    - Confusingly, the "search.action.*" commands are for the Search sidebar panel, they don't work in the Search Editor
  //      - https://github.com/microsoft/vscode/issues/193855 Search Editor: Can't open result with any keybindings, only with double-click
  //    - TODO After they fix this issue, add shift+o for open-to-the-side (like double-click)
  //      - https://github.com/microsoft/vscode/issues/194044 Search Editor: Add keybinding for double-click action, e.g. to open to the side
  {"key": "o", "command": "editor.action.peekDefinition", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Visual.*)$/"},
  {"key": "o", "command": "multiCommand:editor.action.peekDefinition+extension.vim_home+search.action.focusNextSearchResult", "when": "inSearchEditor && !inputBoxFocus && vim.mode == 'Normal'"},
  {"key": "ctrl+]", "command": "editor.action.revealDefinition", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/"}, // Open result in current editor group
  {"key": "enter", "command": "editor.action.revealDefinition", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/"}, // Open result in current editor group
  {"key": "l", "command": "editor.action.revealDefinitionAside", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/"}, // Open result to the side
  {"key": "shift+o", "command": "editor.action.revealDefinitionAside", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/"}, // Open result to the side
  {"key": "shift+enter", "command": "editor.action.revealDefinitionAside", "when": "inSearchEditor && !inputBoxFocus && vim.mode =~ /^(Normal|Visual.*)$/"}, // Open result to the side

  // Navigation
