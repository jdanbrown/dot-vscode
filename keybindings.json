  //    - TODO Fixed once this PR is released! -- https://github.com/microsoft/vscode/pull/235806/files
  //    - [2025-04-25] Tried and failed to figure out editor.action.hideHover (w/ editorHoverVisible/editorHoverFocused), leaving for another day
  {"key": "ctrl+space", "command": "editor.action.showHover", "when": "textInputFocus"}, // Like gh (vim)
  // {"key": "ctrl+space", "command": "editor.action.showHover", "when": "textInputFocus && !hoverVisible"}, // Toggle on (like gh in vim)
  // {"key": "ctrl+space", "command": "editor.action.hideHover", "when": "textInputFocus && hoverVisible"}, // Toggle off
  // {"key": "escape", "command": "editor.action.hideHover", "when": "textInputFocus && hoverVisible && vim.mode == 'Normal'"}, // Don't interfer with escape in Insert/Visual modes
  //  - Parameter hints
  {"key": "shift+cmd+space", "command": "-editor.action.triggerParameterHints", "when": "editorTextFocus && editorHasSignatureHelpProvider"}, // Default
  {"key": "shift+cmd+space", "command": "editor.action.triggerParameterHints", "when": "editorTextFocus && editorHasSignatureHelpProvider && !parameterHintsVisible"}, // Toggle on
  {"key": "shift+cmd+space", "command": "closeParameterHints", "when": "editorTextFocus && parameterHintsVisible"}, // Toggle off
  {"key": "escape", "command": "closeParameterHints", "when": "editorTextFocus && parameterHintsVisible && vim.mode == 'Normal'"}, // Don't interfer with escape in Insert/Visual modes
  //  - Suggest
  {"key": "alt+escape", "command": "-editor.action.triggerSuggest", "when": "textInputFocus && editorHasCompletionItemProvider && !editorReadonly && !suggestWidgetVisible"}, // Default
  {"key": "alt+escape", "command": "editor.action.triggerSuggest", "when": "textInputFocus && editorHasCompletionItemProvider && !suggestWidgetVisible"}, // Toggle on
  {"key": "alt+escape", "command": "hideSuggestWidget", "when": "textInputFocus && suggestWidgetVisible"}, // Toggle off
  {"key": "escape", "command": "hideSuggestWidget", "when": "textInputFocus && suggestWidgetVisible && vim.mode == 'Normal'"}, // Don't interfer with escape in Insert/Visual modes

  // Move lines up/down
  {"key": "ctrl+cmd+j", "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
  {"key": "ctrl+cmd+k", "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly"},
  {"key": "alt+down", "command": "-editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
  {"key": "alt+up", "command": "-editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly"},

  // vim
  //  - Unshadow vim redo
  {"key": "ctrl+r", "command": "-workbench.action.openRecent"},
  //  - shift+esc is a common typo when typing fast with shift
  {"key": "shift+escape", "command": "extension.vim_escape", "when": "vim.active && !inDebugRepl && vim.mode != 'Normal'"},
  //  - TODO TODO
  // {"key": "escape", "command": "highlightwords.addHighlight", "when": "vim.active && !inDebugRepl && vim.mode == 'Normal'"},

  // Multi-cursor
  {"key": "ctrl+j", "command": "editor.action.insertCursorBelow", "when": "editorTextFocus && !inSearchEditor && editorLangId != 'magit'"},
